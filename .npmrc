# NPM Configuration for EasyPanel deployment
# Disable audit warnings during production builds
audit=false
fund=false
# Enable legacy peer deps for better compatibility
legacy-peer-deps=true
# Optimize for production builds
prefer-offline=true
# Cache configuration
# Progress configuration
progress=false
loglevel=warn
# Registry configuration
registry=https://registry.npmjs.org/
# Security configuration
ignore-scripts=false
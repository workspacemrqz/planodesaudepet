{
  "name": "planodesaudepet",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "build": "vite build --config vite.config.prod.ts && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "node server/start-production.js",
    "start:direct": "cross-env NODE_ENV=production node dist/index.js",
    "heroku-postbuild": "npm run build",
    "postinstall": "npm run build",
    "prebuild": "npm run check",
    "check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:auth": "vitest run server/tests/auth.test.ts",
    "test:integrity": "vitest run server/tests/database-integrity.test.ts",
    "test:production": "node scripts/test-production.js",
    "test:config": "node scripts/test-auto-config.js",
    "test:startup": "node scripts/test-startup.js",
    "clean": "rm -rf dist node_modules/.cache",
    "images:validate": "node client/build-config.js",
    "images:check": "npm run images:validate",
    "deploy:build": "npm run clean && npm run build",
    "deploy:docker": "docker build -t planodesaudepet:latest .",
    "deploy:run": "docker run -d --name planodesaudepet -p 8080:8080 -e NODE_ENV=production planodesaudepet:latest",
    "deploy:stop": "docker stop planodesaudepet && docker rm planodesaudepet",
    "deploy:restart": "npm run deploy:stop && npm run deploy:run"
  },
